{
  "name": "dynamic-express-logging",
  "version": "1.0.4",
  "description": "express logging for incoming request and response",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/istanbul cover node_modules/.bin/_mocha ./unit-tests/index-spec.js -- --reporter mochawesome",
    "code-coverage-validation": "istanbul check-coverage --statement 98 --branch 98 --function 98",
