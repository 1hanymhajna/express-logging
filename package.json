  "dependencies": {
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "coveralls": "2.13.1",
    "istanbul": "0.4.5",
    "mocha": "3.4.2",
    "mochawesome": "2.2.0",
    "rewire": "2.5.2",
    "should": "11.2.1",
    "sinon": "2.3.2"
  }
